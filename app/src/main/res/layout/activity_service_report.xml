<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_service_report"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.powerge.wise.powerge.operationProjo.net.view.mview.MyTitle
        android:id="@+id/mt_service_report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.powerge.wise.powerge.operationProjo.net.view.mview.SeachView
        android:id="@+id/sv_service_report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/srl_service_report"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.powerge.wise.powerge.operationProjo.net.view.mview.RefreshRecyclerView
            android:id="@+id/rrv_service_report"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </android.support.v4.widget.SwipeRefreshLayout>
    <com.powerge.wise.powerge.operationProjo.net.view.mview.EmptyView
        android:id="@+id/ev_service_report"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
