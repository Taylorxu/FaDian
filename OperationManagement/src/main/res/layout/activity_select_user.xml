<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_select_user"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wisesignsoft.OperationManagement.view.mview.MyTitle
        android:id="@+id/mt_select_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.wisesignsoft.OperationManagement.view.mview.SeachView
        android:id="@+id/sv_select_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_select_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.wisesignsoft.OperationManagement.view.mview.EmptyView
        android:id="@+id/ev_select_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
